//My solution

function towerBuilder(floors) {
  let arr=[]
  
  for(let i=1;i<=floors;i++){
     let space = " ".repeat(floors - i);
    let star = ("*").repeat((2 * i) - 1);
    arr.push(space + star + space);
  }
  return arr
}
